<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="leftTop" format="dimension" />
    <attr name="leftBottom" format="dimension" />
    <attr name="rightTop" format="dimension" />
    <attr name="rightBottom" format="dimension" />


    <declare-styleable name="VectorClipLayout">
        <attr name="viewportWidth" format="integer" />
        <attr name="viewportHeight" format="integer" />
        <attr name="pathData" format="string" />
    </declare-styleable>

    <declare-styleable name="RoundClipLayout">
        <attr name="android:radius" format="dimension" />
        <attr name="leftTop" />
        <attr name="leftBottom" />
        <attr name="rightTop" />
        <attr name="rightBottom" />
    </declare-styleable>

    <declare-styleable name="ClipLayout">
        <attr name="clipStrokeWidth" format="dimension" />
        <attr name="clipStrokeColor" format="color" />
        <attr name="clipStrokeEnable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SquircleClipLayout">
        <attr name="android:radius" format="dimension" />
        <attr name="leftTop" />
        <attr name="leftBottom" />
        <attr name="rightTop" />
        <attr name="rightBottom" />
        <attr name="radiusWeight" format="float" />
        <attr name="leftTopWeight" format="float" />
        <attr name="leftBottomWeight" format="float" />
        <attr name="rightTopWeight" format="float" />
        <attr name="rightBottomWeight" format="float" />
    </declare-styleable>

</resources>