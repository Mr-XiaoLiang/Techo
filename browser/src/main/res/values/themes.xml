<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Techo" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/brand_4</item>
        <item name="colorPrimaryVariant">@color/brand_7</item>
        <item name="colorOnPrimary">@color/gray_1</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/brand_4</item>
        <item name="colorSecondaryVariant">@color/brand_7</item>
        <item name="colorOnSecondary">@color/gray_1</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@android:color/transparent</item>
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>
</resources>