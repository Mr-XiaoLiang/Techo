<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RecorderWaveView">
        <attr name="lineWidth" format="dimension" />
        <attr name="lineSpace" format="dimension" />
        <attr name="lineColor" format="color" />
    </declare-styleable>
    <declare-styleable name="CheckableView">
        <attr name="checkStyle" format="enum">
            <enum name="circular" value="0" />
            <enum name="square" value="1" />
        </attr>
        <attr name="android:checked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="OverflowScrollBar">
        <attr name="android:color" format="color" />
        <attr name="isHorizontal" format="boolean" />
    </declare-styleable>
    <declare-styleable name="VectorClipLayout">
        <attr name="viewportWidth" format="integer" />
        <attr name="viewportHeight" format="integer" />
        <attr name="pathData" format="string" />
    </declare-styleable>
    <declare-styleable name="RoundClipLayout">
        <attr name="android:radius" format="dimension" />
        <attr name="leftTop" format="dimension" />
        <attr name="leftBottom" format="dimension" />
        <attr name="rightTop" format="dimension" />
        <attr name="rightBottom" format="dimension" />
    </declare-styleable>

    <declare-styleable name="AudioVisualizerView">
        <attr name="barCount" format="integer" />
        <attr name="intervalWeight" format="float" />
        <attr name="baseLineHeight" format="dimension" />
        <attr name="baseLineInterval" format="dimension" />
        <attr name="selectedColor" format="color" />
        <attr name="defaultColor" format="color" />
    </declare-styleable>
</resources>